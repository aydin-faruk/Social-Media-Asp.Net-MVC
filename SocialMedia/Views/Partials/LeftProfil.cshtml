<div class="box box-primary">
    <div class="box-body box-profile">
        <img class="profile-user-img img-responsive img-circle" src="@Session["SNPicture"]" alt="User profile picture">
        <h3 class="profile-username text-center">@Session["SNName"] @Session["SNSurName"]</h3>
        <p class="text-muted text-center">Kou Bilşim Sist. Müh.</p>
        <ul class="list-group list-group-unbordered">
            <li class="list-group-item">
                <b>Takip Edilen</b> <a class="pull-right">@Session["SNFollow"]</a>
            </li>
            <li class="list-group-item">
                <b>Takipçiler</b> <a class="pull-right">0</a>
            </li>
            <li class="list-group-item">
                <b>Gönderi</b> <a class="pull-right">@Session["SNShare"]</a>
            </li>
        </ul>
        <a href="/Profil/Profil" class="btn btn-primary btn-block"><b>Profilim</b></a>
    </div>
    <!-- /.box-body -->
</div>
